{
  "name": "tsc_react_express_app",
  "version": "1.0.0",
  "description": "TypeScript React Express app to training",
  "main": "index.js",
  "scripts": {
    "dev": "npm run format && webpack-dev-server --config ./webpack/webpack.config.js",
    "build": "npm run format && webpack --config ./webpack/webpack.config.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "type-check": "tsc",
    "type-check:watch": "tsc --watch",
    "lint": "npm run type-check && eslint './server/client/src/**/*.{ts,tsx}'",
    "lint:fix": "npm run type-check && eslint './server/client/src/**/*.{ts,tsx}' --fix",
    "format": "npm run lint:fix && prettier --write './server/client/src/**/*.{ts,tsx}' --loglevel 'error'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cruzortiz99/tsc_react_express_app.git"
  },
  "keywords": [
    "typescript",
    "react",
    "express"
  ],
  "author": "Cruz Ortiz",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/cruzortiz99/tsc_react_express_app/issues"
  },
  "homepage": "https://github.com/cruzortiz99/tsc_react_express_app#readme",
  "dependencies": {
    "express": "^4.17.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/express": "^4.17.0",
    "@types/node": "^12.6.6",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "@types/react-router-dom": "^4.3.4",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.1.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "html-webpack-plugin": "^3.2.0",
    "prettier": "^1.18.2",
    "style-loader": "^0.23.1",
    "typescript": "^3.5.3",
    "webpack": "^4.36.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2"
  }
}
